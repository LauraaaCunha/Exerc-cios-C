#include <stdio.h>
#include <stdlib.h>

int main()
{

    int matriz[3][3] = {{3,4,1}, {0,2,7},{10,15,20}};
    int soma=0, i, j;


    /*printf("valores matriz: ");
    for( i=0; i<3; i++)
    {
        for( j=0; j<3; j++)
        {
            scanf("%d", &matriz[i][j]);

        }
    }*/


    //soma linhas
     for( i=0; i<3; i++)
    {
        soma = 0;
        for( j=0; j<3; j++)
        {
             soma = soma + matriz[i][j];

        }
        printf("soma linha: %d \n", soma);
    }

        //soma colunas
     for( j=0; j<3; j++)
    {
        soma = 0;
        for( i=0; i<3; i++)
        {
             soma = soma + matriz[i][j];

        }
        printf("soma colunas: %d \n", soma);
    }



}
