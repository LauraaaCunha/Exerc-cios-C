#include <stdio.h>
#include <stdlib.h>

int funcao(int tam, int vetor[])
{

    int soma=0;

    for(int j=0; j<tam; j++)
    {
        soma = soma + vetor[j];
    }
    return soma;

}



int main()
{

    int vetor_a[5];

    for(int j=0; j<5; j++)
    {
        printf("valores: ");
        scanf("%d", &vetor_a[j]);
    }

    printf("%d", funcao(5, vetor_a));

    return 0;

}
